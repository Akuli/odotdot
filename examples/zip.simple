# currently there's not a better way to define functions than this...
# array_func creates a function that takes any number of arguments and
# puts them to a local arguments array

var array1 = (Array "a" "b" "c");
var array2 = (Array "d" "e" "f");

assert (equals (zip array1 array2) (Array (Array "a" "d") (Array "b" "e") (Array "c" "f")));
(zip array1 array2).foreach "item" {
    print (item.get "0");
    print (item.get "1");
};
