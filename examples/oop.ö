var Tooter = (class {
    var setup = (meth "asd" {
        this.asd = asd;
    });

    var toot = (meth {
        print this.asd;
    });
});


var FancyTooter = (class inheriting Tooter {
    var fancytoot = (meth {
        print "*** Extra Fanciness ***";
        this::toot;
    });
});


var t = (new Tooter "Toot Toot!");
print "tooting:";
t::toot;

print "----------";

var t2 = (new FancyTooter "Toot Toot!");
print "tooting the fancy toot:";
t2::fancytoot;
