class "Tooter" {
    attrib "asd";

    method "setup asd" {
        this.asd = asd;
    };

    method "toot" {
        print this.asd;
    };
};


class "FancyTooter"
    inherits: Tooter
{
    method "fancytoot" {
        print "*** Extra Fanciness ***";
        this.toot;
    };
};


var t = (new Tooter "Toot Toot!");
print "tooting:";
t.toot;

print "----------";

var t2 = (new FancyTooter "Toot Toot!");
print "tooting the fancy toot:";
t2.fancytoot;
