var stacks = (import "<stdlibs>/stacks");

test "StackFrame" {
    assert (stacks.StackFrame.name `equals` "StackFrame");
};

test "get_stack" {
    var stack = (stacks.get_stack);
    var last_frame = (stack.get (stack.length.plus -1));
    assert (last_frame `is_instance_of` StackFrame);
    assert (last_frame.filename `ends_with` "test_errors.รถ");
};
