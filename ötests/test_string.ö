var split_by_ws_tests = [
    [" asd toot " ["asd" "toot"]]
    ["  asd  toot  " ["asd" "toot"]]
    ["  asd  toot" ["asd" "toot"]]
    ["asd  toot  " ["asd" "toot"]]
    ["asd  toot" ["asd" "toot"]]
    ["  " []]
    [" " []]
    ["" []]
];

foreach "test" split_by_ws_tests {
    var input = (test::get 0);
    var expected_output = (test::get 1);
    assert ((input::split_by_whitespace) `equals` expected_output);
};
